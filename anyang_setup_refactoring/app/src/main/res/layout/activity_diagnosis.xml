                    android:layout_marginLeft="20dp"
                    android:gravity="center"
                    android:text="졸업학점"
                    android:textSize="17dp" />


            </RelativeLayout>

            <TableLayout
                android:id="@+id/tableLayout"
                android:layout_width="390dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:stretchColumns="*">

                <TableRow>

                    <TextView
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="구분"
                        android:textStyle="bold" />

                    <TextView
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="기준학기 수"
                        android:textStyle="bold" />

                    <TextView
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="등록/이수학기 수"
                        android:textStyle="bold" />

                </TableRow>

                <TableRow>

                    <TextView
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="등록" />

                    <TextView
                        android:id="@+id/t1"
                        android:gravity="center"
                        android:padding="8dp" />

                    <TextView
                        android:id="@+id/t2"
                        android:gravity="center"
                        android:padding="8dp" />
                </TableRow>

                <TableRow>

                    <TextView
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="채플이수" />

