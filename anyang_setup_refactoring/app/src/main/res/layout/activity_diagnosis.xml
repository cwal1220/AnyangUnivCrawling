            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <WebView
                android:id="@+id/webview_diagnosis_2"
                android:layout_width="390dp"
                android:layout_height="390dp"
                android:layout_marginTop="10dp" />

            <TextView
                android:id="@+id/Explanation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="상단에 표시된 인트라넷에서 \n 졸업 -> 졸업자가진단까지 눌러주시고\n하단의 진단버튼을 눌러주세요 \n앱에서 졸업자가진단을 분석하여 해설해드립니다"
                android:textAlignment="center"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/python_result_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Python Result"
                android:layout_marginTop="16dp"/>


            <Button
                android:id="@+id/Update_button_2"
                android:layout_width="110dp"
                android:layout_height="70dp"
                android:layout_marginTop="10dp"
                android:text="진단" />


            <RelativeLayout
