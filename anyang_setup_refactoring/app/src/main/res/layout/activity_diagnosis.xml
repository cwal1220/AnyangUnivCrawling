                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:gravity="center"
                    android:text="복수전공"
                    android:textSize="17dp" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Linked_Major_Layout"
                android:layout_width="390dp"
                android:layout_height="90dp"
