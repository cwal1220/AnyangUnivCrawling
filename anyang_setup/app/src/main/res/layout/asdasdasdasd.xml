                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:background="@drawable/shape_corners">

                <TableLayout
                    android:id="@+id/tableLayout2"
                    android:layout_width="390dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="20dp"
                    android:stretchColumns="*">

                    <TableRow>

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="구분"
                            android:textStyle="bold" />

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="과목명"
                            android:textStyle="bold"/>

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="이수여부"
                            android:textStyle="bold"/>


                    </TableRow>

                    <TableRow>

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="교양필수" />

                        <TextView
                            android:id="@+id/General_Education_Requirement1_name"
                            android:gravity="center"
                            android:padding="8dp" />

                        <TextView
                            android:id="@+id/General_Education_Requirement1_status"
                            android:gravity="center"
                            android:padding="8dp" />


                    </TableRow>

                    <TableRow>

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="교양필수" />

                        <TextView
                            android:id="@+id/General_Education_Requirement2_name"
                            android:gravity="center"
                            android:padding="8dp" />

                        <TextView
                            android:id="@+id/General_Education_Requirement2_status"
                            android:gravity="center"
                            android:padding="8dp" />


                    </TableRow>

                    <TableRow>

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="교양필수" />

                        <TextView
                            android:id="@+id/General_Education_Requirement3_name"
                            android:gravity="center"
                            android:padding="8dp" />

                        <TextView
                            android:id="@+id/General_Education_Requirement3_status"
                            android:gravity="center"
                            android:padding="8dp" />


                    </TableRow>

                    <TableRow>

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="교양필수" />

                        <TextView
                            android:id="@+id/General_Education_Requirement4_name"
                            android:gravity="center"
                            android:padding="8dp" />

                        <TextView
                            android:id="@+id/General_Education_Requirement4_status"
                            android:gravity="center"
                            android:padding="8dp" />


                    </TableRow>

                    <TableRow>

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="교양필수" />

                        <TextView
                            android:id="@+id/General_Education_Requirement5_name"
                            android:gravity="center"
                            android:padding="8dp" />

                        <TextView
                            android:id="@+id/General_Education_Requirement5_status"
                            android:gravity="center"
                            android:padding="8dp" />


                    </TableRow>

                    <TableRow>

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="교양필수" />

                        <TextView
                            android:id="@+id/General_Education_Requirement6_name"
                            android:gravity="center"
                            android:padding="8dp" />

                        <TextView
                            android:id="@+id/General_Education_Requirement6_status"
                            android:gravity="center"
                            android:padding="8dp" />


                    </TableRow>

                    <TableRow>

                        <TextView
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="교양필수" />

                        <TextView
                            android:id="@+id/General_Education_Requirement7_name"
                            android:gravity="center"
                            android:padding="8dp" />

                        <TextView
                            android:id="@+id/General_Education_Requirement7_status"
                            android:gravity="center"
                            android:padding="8dp" />


                    </TableRow>


                </TableLayout>

            </RelativeLayout>

            <TextView
                android:id="@+id/General_Education_Requirement5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="-50dp"
                android:gravity="center"
                android:text="-- 과목을 이수 하셔야 합니다."
                android:textSize="17dp" />

            <RelativeLayout
                android:id="@+id/General_Education_Requirement_Layout3"
                android:layout_width="390dp"
                android:layout_height="30dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:background="@drawable/shape_corners">

            </RelativeLayout>

            <TextView
                android:id="@+id/General_Education_Requirement8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
