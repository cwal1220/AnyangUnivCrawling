<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".test_Spec">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <WebView
                android:id="@+id/webview_diagnosis_2"
                android:layout_width="390dp"
                android:layout_height="46dp"
                android:layout_marginTop="10dp" />


            <Button
                android:id="@+id/Update_button_2"
                android:layout_width="110dp"
                android:layout_height="70dp"
                android:layout_marginTop="10dp"
                android:text="진단" />

            <RelativeLayout
                android:id="@+id/General_Education_Requirement_Layout"
                android:layout_width="390dp"
                android:layout_height="90dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:background="@drawable/shape_corners">

                <TextView
                    android:id="@+id/General_Education_Requirement"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:gravity="center"
                    android:text="취득학점/ 121"
                    android:textSize="17dp" />

                <TextView
                    android:id="@+id/General_Education_Requirement2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="170dp"
                    android:gravity="center"
                    android:text="졸업학점/ 130"
                    android:textSize="17dp" />

            </RelativeLayout>

            <TextView
                android:id="@+id/General_Education_Requirement3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="-50dp"
                android:gravity="center"
                android:text="현재 9 학점이 부족합니다."
                android:textSize="17dp" />

            <TextView
                android:id="@+id/General_Education_Requirement4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="-50dp"
                android:gravity="center"
                android:text="--학년 평균 취득 학점은 --입니다."
                android:textSize="17dp" />

            <RelativeLayout
                android:id="@+id/General_Education_Requirement_Layout2"
                android:layout_width="390dp"
