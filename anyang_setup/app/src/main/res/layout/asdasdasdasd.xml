                    android:stretchColumns="*">

                    <TableRow>

                        <TextView
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="구분"
                            android:textStyle="bold" />

                        <TextView
                            android:gravity="center"
                            android:padding="3dp"
